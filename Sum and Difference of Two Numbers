#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
	int n=10, m=4, sum, diff;
    float p=4.0, q=2.0, sum1, diff1;
    scanf("%d %d\n", &n,&m);
    scanf("%f %f\n", &p,&q);
    sum=n+m;
    diff=n-m;
    printf("%d %d\n",sum,diff);
    sum1=p+q;
    diff1=p-q;
    printf("%0.1f %0.1f\n",sum1,diff1);
    
    return 0;
}
